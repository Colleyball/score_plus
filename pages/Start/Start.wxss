page,.page {
  font-family: 'PingFang SC', 'Helvetica Neue' ,Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
  background:url('http://www.volleywang.cn/liansaiquan/images/matchbox/sb/bg.png') no-repeat;
  background-size: 100%;
  background-position: bottom;
  position: absolute;
  background-attachment: fixed;
}

.top{
  width: 650rpx;
  height: 150rpx;
  line-height: 150rpx;
  margin-left: auto;
  margin-right: auto;
  box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.2);
  background: url('http://127.0.0.1/scoreplus/title-bg.png') center no-repeat;
  background-size: cover;
  text-align: left;
  padding-left: 30rpx;
  font-size: 45rpx;
  margin-bottom: 40rpx;
  color: #1A1A1A
}

.resultlist{
  margin-top: 20rpx;
}


.result{
  position: relative;
  width: 680rpx;
  height:250rpx;
  background-color: white;
  padding: 0;
  border-radius: 0rpx;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 40rpx 35rpx;
  text-align: center;
}

.teamA{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
}

.teamB{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
}

.vs{
  width: 100%;
  height: 70rpx;
  background-color: #3369E8;
  line-height:70rpx;
  margin-top: 0rpx;
  margin-left: auto;
  margin-right: auto;
  color: white;
  font-size: 35rpx;
}

.tenniscourt {
  width: 680rpx;
  height: 305rpx;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 40rpx 35rpx;
  text-align: center;
}

.courtarea {
  height:235rpx;
  display: flex;
  flex-direction: row;
}

.halfcourt {
  width:340rpx;
  height:100%;
  display:flex;
  flex-direction:row;
  border:1rpx solid black
}

.left-server {
  background: url(http://127.0.0.1/scoreplus/tennis.png) no-repeat;
  background-size: 60rpx;
  background-position: center;
  width:100%;
  height: 50%;
  margin-top: 117.5rpx
}

.right-server {
  background: url(http://127.0.0.1/scoreplus/tennis.png) no-repeat;
  background-size: 60rpx;
  background-position: center;
  width:100%;
  height: 50%;
}

.frontcourt {
  width: 183rpx;
  height:100%;
  display:flex;
  flex-direction:column;
  border-left: 1rpx solid black
}

.right-frontcourt {
  width: 183rpx;
  height:100%;
  display:flex;
  flex-direction:column;
  border-right: 1rpx solid black
}

.tablename{
  width: 680rpx;
  height: 70rpx;
  margin-left: 35rpx;
  background-color: #3369E8;
  line-height:70rpx;
  margin-top: 0rpx;
  text-align: center;
  color: white;
  font-size: 35rpx;
}

.tableinfo{
  position: relative;
  width: 680rpx;
  height:90rpx;
  line-height: 90rpx;
  background-color: white;
  padding: 0;
  border-radius: 0rpx;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 40rpx 35rpx;
  text-align: center;
}

.submit{
 border: 2px solid rgba(0, 0, 0, 0.2);
 border-radius: 0px;
 margin-top: 15px;
 width:680rpx;
 height:100rpx;
 margin-left: 35rpx;
 line-height: 100rpx;
 background-color: rgba(241, 124, 103, 0.8);
 box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
 color: white
}

